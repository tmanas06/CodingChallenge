{
  "name": "interactive-concept-visualization-tool",
  "version": "1.0.0",
  "description": "JavaScript Interactive Concept Visualization Tool with Canvas API, WebGL, and SVG",
  "main": "src/main.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "webpack --mode production",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:performance": "node tests/performance.test.js",
    "test:all": "npm run test && npm run test:performance",
    "demo": "node demo.js",
    "load-test": "node tests/load-test.js",
    "benchmark": "node tests/benchmark.js",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix"
  },
  "keywords": [
    "visualization",
    "learning",
    "canvas",
    "webgl",
